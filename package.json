{
  "name": "tsx-test",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "start": "run-p start:*",
    "start:router": "node dist/router",
    "start:tailor": "node dist/tailor",
    "start:fragment": "run-p start:fragment:*",
    "start:fragment:test": "node fragments/fragment",
    "start:fragment:category": "node dist/fragments/category",
    "build": "run-p build:*",
    "build:tailor": "webpack --config webpack/tailor --mode development",
    "build:router": "webpack --config webpack/router --mode development",
    "build:fragment": "run-p build:fragment:*",
    "build:fragment:category": "webpack --config webpack/fragments/category --mode development"
  },
  "license": "MIT",
  "dependencies": {
    "node-tailor": "^3.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/node": "^9.4.7",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "express": "^4.16.3",
    "nodemon": "^1.17.2",
    "npm-run-all": "^4.1.2",
    "ts-loader": "^4.0.1",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-node-externals": "^1.6.0",
    "webpack-serve": "^0.2.0"
  }
}
