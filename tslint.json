{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "max-line-length": false,
    "indent": [
      true,
      "spaces"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "no-var-requires": false,
    "ordered-imports": false,
    "member-ordering": [
      false
    ],
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "no-console": [
      false
    ],
    "jsx-alignment": [
      false
    ],
    "no-submodule-imports": [
      true,
      "react-dom/server"
    ]
  }
}